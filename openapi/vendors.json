            "type": "string"
          },
          "apiKey": {
            "type": "string"
          },
          "backendStack": {
            "type": "object"
          },
          "frontendStack": {
            "type": "object"
          },
          "typeDB": {
            "type": "string"
          },
          "logo": {
            "type": "string"
          },
          "icon": {
            "type": "string"
          },
          "dateFormat": {
            "type": "string"
          },
          "startWeekDay": {
            "type": "string"
          },
          "timeFormat": {
            "type": "string"
          },
          "timezone": {
            "type": "string"
          },
          "currency": {
